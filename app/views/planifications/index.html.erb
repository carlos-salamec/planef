<p id="notice"><%= notice %></p>

<h1>Descargar Planificaciones</h1>

<%= render 'search_form' %>

<table>
  <thead>
    <tr>
      <th>Título</th>
      <th>Asignatura</th>
      <th>Curso</th>
      <th>Descargas</th>
      <th>Creador original:</th>
      <th>Última edición por:</th>
      <th>Rating</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @planifications.each do |plan| %>
      <%= render partial: 'plan_box', locals: {plan: plan} %>
    <% end %>
  </tbody>
</table>

<br>
